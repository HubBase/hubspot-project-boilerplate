{
  "name": "hubspot-cms-helloworld",
  "version": "1.0.0",
  "description": "A helloworld to demonstrate HubSpot CMS local development",
  "main": "index.js",
  "repository": "https://github.com/umair-khokhar/hubspot-cms-helloworld",
  "author": "Umair Khokhar",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint src",
    "lint": "eslint src && prettier --list-different 'src/**/*.js' 'src/**/*.json'",
    "prettier:write": "prettier --write 'src/**/*.js' 'src/**/*.json'",
    "watch": "npx hs watch src theme2020",
    "watch-remove": "npx hs watch src theme2020 --remove",
    "fetch": "npx hs fetch theme2020 src --overwrite",
    "fetch-no-overwrite": "npx hs fetch theme2020 src"
  },
  "devDependencies": {
    "@hubspot/cms-cli": "^1.0.2",
    "eslint": "^6.3.0",
    "js-yaml": "^3.13.1"
  },
  "dependencies": {
    "nvm": "0.0.4"
  }
}
